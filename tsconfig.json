{
	"compilerOptions": {
		"target": "ES3",
		"ignoreDeprecations": "5.0",
		"moduleResolution": "node",
		"noLib": true,
		"strict": true,
		"types": ["shared/ScriptUI.d.ts", "jsx.d.ts"],
		"typeRoots": ["./node_modules/types-for-adobe/", "./src"],
		"jsx": "react",
		"jsxFactory": "jsx"
	},
	"include": ["src/**/*.ts", "src/**/*.tsx"],
	"exclude": ["dist"]
}
